import { storyUrl } from '@cld/storybook.macro';
import { e2eAdapter, testAdapter } from '@cld/e2e-adapter';
import { createIconNextDriver } from '../index';

describe('generic next', () => {

  it('should check generic icon', async () => {
    const driver = createIconNextDriver(e2eAdapter, { specifier: '[data-test-specifier="generic-icon"]' });
    await testAdapter.log('Navigate to test page');
    await testAdapter.navigateTo(storyUrl('GenericIcon/default'));
    await testAdapter.log('Check element');
    await driver.compareWithScreenshot('generic icon');
  });

});
